<center>
    <table border="1">
        <% themerecord.map((val)=>{ %>
            <tr>
                <td><%= val.name %></td>
                <td><img src="<%= val.img %>" width="100"/></td>
                <td><%= val.price %></td>
                <td><%= val.category %></td>
                <td><%= val.description %></td>
                <td>
                    <% val.images.map((item)=>{ %>
                        <img src="<%= item %>" width="100"/>
                    <% }) %>
                    
                </td>
                
            </tr>
        <% }) %>
    </table>
</center>